          anchor_ranges: [-49.6, -49.6, -0.36061722, 49.6, 49.6, -0.36061722]
          rotations: [0, 1.57] # DON'T modify this unless you are very familiar with my code.
          # custom_values: [0, 0] # velocity vector base value
        }
